{
  "name": "@code-sugar/slidev-theme-portrait",
  "version": "0.0.0",
  "type": "module",
  "keywords": [
    "slidev-theme",
    "slidev"
  ],
  "engines": {
    "node": ">=18.0.0",
		"slidev": ">=0.48.0"
  },
  "scripts": {
    "build": "slidev build example.md",
    "dev": "slidev example.md --open",
    "export": "slidev export example.md",
    "screenshot": "slidev export example.md --format png"
  },
  "dependencies": {
    "@slidev/types": "^51.0.2"
  },
  "devDependencies": {
    "@slidev/cli": "^51.0.2"
  },
  "slidev": {
    "colorSchema": "both",
    "defaults": {
			"aspectRatio": "9/16",
			"layout": "portrait",
			"codeCopy": false,
      "fonts": {
        "sans": "Nunito Sans",
        "mono": "Fira Code"
			}
    }
  }
}