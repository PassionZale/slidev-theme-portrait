{
  "name": "@code-sugar/slidev-theme-portrait",
  "version": "0.0.2",
  "type": "module",
  "description": "A slide portrait theme for record portrait video.",
  "author": "Lei Zhang",
  "email": "whouu@qq.com",
  "license": "MIT",
  "homepage": "https://github.com/PassionZale/slidev-theme-portrait#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PassionZale/slidev-theme-portrait.git"
  },
  "bugs": "https://github.com/PassionZale/slidev-theme-portrait/issues",
  "keywords": [
    "slidev-theme",
    "slidev"
  ],
  "engines": {
    "node": ">=18.0.0",
    "slidev": ">=0.48.0"
  },
  "scripts": {
    "build": "slidev build example.md",
    "dev": "slidev example.md --open",
    "export": "slidev export example.md",
    "screenshot": "slidev export example.md --format png",
    "release": "npm publish --registry https://registry.npmjs.org --access public"
  },
  "dependencies": {
    "@slidev/types": "^51.0.2"
  },
  "devDependencies": {
    "@slidev/cli": "^51.0.2"
  },
  "slidev": {
    "colorSchema": "both",
    "defaults": {
      "aspectRatio": "9/16",
			"canvasWidth": 720,
      "layout": "portrait",
      "codeCopy": false,
      "fonts": {
        "sans": "Nunito Sans",
        "mono": "Fira Code"
      }
    }
  }
}
